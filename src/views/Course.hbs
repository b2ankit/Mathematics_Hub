    if (document.body.scrollTop > 80 || document.documentElement.scrollTop > 80) {
      document.getElementById("nav").style.backgroundColor = "white";
      document.getElementById("hed1").style.color = "black";
      $(".bar a").css("color", "black");
      $("#toggle").css("color","black");
        } else {
        document.getElementById("nav").style.backgroundColor = "#373a6d";
        document.getElementById("hed1").style.color = "white";
        $(".bar a").css("color", "white");
        $("#toggle").css("color","white");
        }
    }
</script>
